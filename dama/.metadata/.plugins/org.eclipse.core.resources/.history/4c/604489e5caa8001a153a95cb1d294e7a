package dominio;

public class Casella {
	private int riga;
	private int colonna;
	private int simbolo;
	private static Casella singleton;
	
	public static Casella getInstance() {
		if (singleton==null)
			singleton=new Casella();
		return singleton;
	}


	public int getRiga() {
		return riga;
	}

	public void setRiga(int riga) {
		this.riga = riga;
	}

	public int getColonna() {
		return colonna;
	}

	public void setColonna(int colonna) {
		this.colonna = colonna;
	}

	public int getSimbolo() {
		return simbolo;
	}

	public void setSimbolo(int simbolo) {
		this.simbolo = simbolo;
	}

}
